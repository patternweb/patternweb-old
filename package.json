{
  "name": "patternweb",
  "version": "0.0.1",
  "author": "John Rees <john@bitsushi.com>",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest",
    "lint": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "build": "cp -R public dist && PRODUCTION=true webpack -p",
    "deploy": "npm run build && ghpages -p dist && rm -rf dist"
  },
  "keywords": [],
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/enzyme": "^3.1.5",
    "@types/jest": "^21.1.8",
